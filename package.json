  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "if-env": "^1.0.4",
    "serve": "^14.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1"
  }
}
